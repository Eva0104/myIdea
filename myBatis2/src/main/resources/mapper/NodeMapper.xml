<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.zhuxiaoxue.mapper.NodeMapper">
    <select id="findById" parameterType="int" resultType="Node">
        SELECT * FROM t_node WHERE id=#{id}
    </select>

    <select id="findAll" resultType="Node">
        SELECT * FROM t_node
    </select>
</mapper>